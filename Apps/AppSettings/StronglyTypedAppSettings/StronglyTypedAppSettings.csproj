<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateProgramFile>false</GenerateProgramFile>
    <LangVersion>latest</LangVersion>
    <EnforceExtendedAnalyzerRules>true</EnforceExtendedAnalyzerRules>
    <OutputType>Library</OutputType>
    <IsRoslynSourceGenerator>true</IsRoslynSourceGenerator>

    <!-- Enable XML documentation generation -->
    <GenerateDocumentationFile>true</GenerateDocumentationFile>

    <!-- NuGet Package Metadata -->
    <PackageId>StronglyTypedAppSettings</PackageId>
    <Version>1.0.5</Version>
    <Authors>ShanieMoonlight</Authors>
    <Description>A source generator for strongly-typed access to appsettings.json.</Description>
    <PackageTags>source-generator;appsettings;configuration</PackageTags>
    <RepositoryUrl>https://github.com/YourRepo/StronglyTypedAppSettings</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/shanieMoonlight/VsExtensions/tree/master</PackageProjectUrl>
    <IncludeBuildOutput>false</IncludeBuildOutput>
    <PrivateAssets>all</PrivateAssets>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Title>Strongly Typed AppSettings</Title>
    <PackageIcon>icon.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.Analyzers" Version="3.11.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" PrivateAssets="all" Version="4.13.0" />
  </ItemGroup>

  <!-- Include the DLL as an analyzer -->
  <ItemGroup>
    <None Include="$(OutputPath)$(AssemblyName).dll" Pack="true" PackagePath="analyzers/dotnet/cs" />
    <None Include="$(OutputPath)$(AssemblyName).xml" Pack="true" PackagePath="analyzers/dotnet/cs" />
  </ItemGroup>

  <!-- Include the local Newtonsoft.Json assembly in the NuGet package -->

  <!--<PropertyGroup>
    <GetTargetPathDependsOn>$(GetTargetPathDependsOn);GetDependencyTargetPaths</GetTargetPathDependsOn>
  </PropertyGroup>-->

